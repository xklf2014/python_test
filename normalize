#规范字符串首字母大写，其他小写


from functools import reduce

def normalize(l):
        U = []
        for s in l:
                lowerst=''.join(map(str,s[1:-1])).lower()
                upperst=''.join(s[0]).upper()
                U.append(upperst+lowerst)
        return U

L1 = ['asdsVd','CsdaCDD','asdadSDFDS']
print(normalize(L1))
